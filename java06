public class NumerosUmAbaixoDoOutro {
    public static void main(String[] args) {
        for (int i = 1; i <= 20; i++) {
            System.out.println(i);
        }
    }
}

public class NumerosUmAoLadoDoOutro {
    public static void main(String[] args) {
        for (int i = 1; i <= 20; i++) {
            System.out.print(i + " ");
        }
    }
}

